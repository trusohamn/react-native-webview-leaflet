{
  "name": "@trusohamn/react-native-webview-leaflet",
  "version": "5.0.2",
  "description": "A React Native component that uses a React Native WebView to provide a Leaflet map.",
  "repository": {
    "type": "git",
    "url": "https://github.com/trusohamn/react-native-webview-leaflet"
  },
  "keywords": [
    "react-native",
    "leaflet",
    "expo",
    "webview"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.json && yarn copy-assets",
    "prepare": "rm -rf lib && yarn build",
    "copy-assets": "cp -rv assets/ lib/assets/",
    "pub": "npm publish --access public"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/lodash.isequal": "^4.5.5",
    "@types/react-native": "^0.63.32",
    "@types/react-leaflet": "^2.4.0",
    "expo-asset": "^7.0.0",
    "expo-asset-utils": "^1.2.0",
    "expo-file-system": "^8.0.0",
    "leaflet": "^1.5.1",
    "lodash.isequal": "^4.5.0",
    "react-dom": "^16.10.2",
    "react-leaflet": "^2.6.1",
    "react-native-webview": "^10.10.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-native": "^0.61.2"
  },
  "devDependencies": {
    "@types/node": "^13.1.6",
    "typescript": "~3.9.5"
  }
}
